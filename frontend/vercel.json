{
    "routes": [
      {
        "src": "/(.*).html",
        "dest": "/$1.html"
      },
      {
        "src": "/(.*).js",
        "dest": "/$1.js"
      },
      {
        "src": "/(.*).css",
        "dest": "/$1.css"
      },
      {
        "src": "/(.*).(jpg|jpeg|gif|png)",
        "dest": "/$1.$2"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }